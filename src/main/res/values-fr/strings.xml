	<string name="pref_summary_default_update_interval">En quelques secondes. Définit à quelle fréquence l\'alarme va chercher les données de l\'échange. Vous pouvez aussi affiner les réglages d\'une alarme particulière.</string>
	<string xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" name="default_value">Défaut (%1$s)<xliff:g id="value" example="Hassium">(%1$s)</xliff:g></string>
	<string xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" name="price_change_firing_reason">%1$s en %2$s<xliff:g id="change_value" example="+10%">%1$s</xliff:g> in <xliff:g id="timespan" example="10m">%2$s</xliff:g></string>
