	<string name="short_description">比特币，数字货币，数字资产以及衍生品的价格提醒。</string>
	<string xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2" name="not_bound">%1$s没有绑定StorageAndControlService。<xliff:g id="class_name" example="AlarmListActivity">%1$s</xliff:g> not bound to StorageAndControlService.</string>
